<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="icon" type="image/png" href="{{ asset('build/images/logoDS.png') }}">
		<link rel="stylesheet" href="/build/app.css">
		<script src="/build/app.js"></script>

		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		<header>
			{% include('_navbar.html.twig') %}
		</header>

		{% block body %}{% endblock %}

		
	</body>
</html>
